# Example environment variables for StuInfoManagementSystem
# Copy to .env and fill values for your environment before running

# Django
DJANGO_SECRET_KEY=replace-this-secret
DJANGO_DEBUG=False

# Database (MySQL)
DB_NAME=studentinfo
DB_USER=root
DB_PASSWORD=123456
DB_HOST=127.0.0.1
DB_PORT=3306

# Testing: set to any value to force using sqlite in-memory for tests
# DJANGO_USE_SQLITE_FOR_TESTS=1
 
# Hosts allowed when DEBUG is False (comma-separated)
ALLOWED_HOSTS=127.0.0.1,localhost
